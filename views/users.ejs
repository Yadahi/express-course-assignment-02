<%- include('includes/head.ejs') %>

<link rel="stylesheet" href="/css/users.css">
</head>

  <body>
    <%- include('includes/navigation.ejs') %>
    <div class="content">
        
        <h1>Users</h1>
        <div>
          <% if (usersArray.length > 0) { %>
          <ul class="users__list">
            <% for (let user of usersArray) { %>
            <li class="users__item"><%= user.name %></li>
            <% } %>
          </ul>
    
          <% } else { %>
          <h1>No users</h1>
          <% } %>
        </div>
    </div>
  </body>
</html>
